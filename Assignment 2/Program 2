#include <stdio.h>
#include <conio.h>
void main()
{
int a,b,c;
printf("enter 1side lengt of triangle",);
scanf("% d",&a); 
printf("enter 1side lengt of triangle",);
scanf("% d",& b); 
printf("enter 1side lengt of triangle",);
scanf("% d",& c); 
if (a==b&b==c)
{
    printf("triangle is equilateral");
    break;
    }
    else if (a==b||a==c||b==c);
    {
    printf("triangle is isosceles");
    break;
    }
    else 
    {
        printf("triangle is scalane");
    }
}

